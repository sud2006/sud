//iki sayının büyüklük küçüklük ilişkisini c kodu ile yazınız.
#include <stdio.h>

int main()
{ 
    int a,b;
    printf("iki sayı giriniz:\n");
    scanf("%d%d",&a,&b);
    if(a>b)
    {
        printf("%d>%d",a,b);
    }
    else if(b>a)
    {
        printf("%d>%d",b,a);
    }
    if(a==b)
    {
        printf("%d=%d",a,b);
    }
    return 0;
}

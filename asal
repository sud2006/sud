//sayıya kadar olan asal sayıları yazdır.

#include <stdio.h>

int main()
{   int sayi,i,m,asal;

    printf("bir sayı giriniz\n");
    scanf("%d",&sayi);
  
   
       for(i=2; i<=sayi; i++){
               asal=1;
           
       
       for(m=2; m<i; m++){
           if(i%m==0){
               asal=0;
               break;
           }
       }
          if(asal==1){
   printf("%d\n",i);
          }
   
       }
   
   
   
    return 0;
}
